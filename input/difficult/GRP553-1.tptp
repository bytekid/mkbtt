cnf(single_axiom,axiom,
    ( divide(divide(A,inverse(divide(B,divide(A,C)))),C) = B )).

cnf(multiply,axiom,
    ( multiply(A,B) = divide(A,inverse(B)) )).

cnf(prove_these_axioms_1,negated_conjecture,
    (  multiply(inverse(a1),a1) != multiply(inverse(b1),b1) )).

