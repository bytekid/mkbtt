cnf(equation_H55,axiom,
    ( join(X,meet(Y,join(X,Z))) = join(X,meet(Y,join(Z,meet(X,join(Z,Y))))) )).

cnf(prove_H59,negated_conjecture,
    (  meet(a,meet(join(b,c),join(b,d))) != meet(a,join(b,meet(join(b,d),join(c,meet(a,b))))) )).

