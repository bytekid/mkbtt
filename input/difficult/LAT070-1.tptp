%----Single axiom OL-23A
cnf(ol_23A,axiom,
    ( f(f(f(f(B,A),f(A,C)),D),f(A,f(f(A,f(f(B,B),B)),C))) = A )).

%----Denial of Sheffer stroke associativity
cnf(associativity,negated_conjecture,
    (  f(a,f(f(b,c),f(b,c))) != f(c,f(f(b,a),f(b,a))) )).

