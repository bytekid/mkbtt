cnf(equation_H21,axiom,
    ( join(meet(X,Y),meet(X,Z)) = meet(X,join(meet(Y,join(X,meet(Y,Z))),meet(Z,join(X,Y)))) )).

cnf(prove_H10,negated_conjecture,
    (  meet(a,join(b,meet(a,c))) != meet(a,join(b,meet(c,join(a,meet(b,c))))) )).

