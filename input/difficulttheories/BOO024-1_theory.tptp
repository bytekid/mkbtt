cnf(multiply_add, axiom, ( multiply(add(X,Y),Y) = Y )).

cnf(multiply_add_property, axiom, ( multiply(X,add(Y,Z)) = add(multiply(Y,X),multiply(Z,X)) )).

cnf(additive_inverse, axiom, ( add(X,inverse(X)) = n1 )).

cnf(pixley_defn, axiom, ( pixley(X,Y,Z) = add(multiply(X,inverse(Y)),add(multiply(X,Z),multiply(inverse(Y),Z))) )).

cnf(pixley1, axiom, ( pixley(X,X,Y) = Y )).

cnf(pixley2, axiom, ( pixley(X,Y,Y) = X )).

cnf(pixley3, axiom, ( pixley(X,Y,X) = X )).

