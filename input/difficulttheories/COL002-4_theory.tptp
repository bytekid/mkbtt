cnf(s_definition, axiom, ( apply(apply(apply(s,X),Y),Z) = apply(apply(X,Z),apply(Y,Z)) )).

cnf(b_definition, axiom, ( apply(apply(apply(b,X),Y),Z) = apply(X,apply(Y,Z)) )).

cnf(c_definition, axiom, ( apply(apply(apply(c,X),Y),Z) = apply(apply(X,Z),Y) )).

cnf(i_definition, axiom, ( apply(i,X) = X )).

cnf(weak_fixed_point, axiom, ( weak_fixed_point(X) = apply(apply(apply(s,apply(b,X)),i),apply(apply(s,apply(b,X)),i)) )).

